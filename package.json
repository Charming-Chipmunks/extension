{
  "name": "chipmunks",
  "version": "1.0.0",
  "description": "Opportunities, a career coaching app",
  "main": "index.js",
  "scripts": {
    "start-webpack": "webpack --config webpack.config.js --progress --watch",
    "build": "npm install && webpack",
    "start": "nodemon server/server.js 2>&1 | log/.predate.sh | tee -a log/server.log ",
    "start-blind": "nodemon server/server.js 2>&1 | log/.predate.sh >> log/server.log & ",
    "stop": "killall -9 node",
    "tail": "tail -f log/server.log",
    "run-ios": "cd Phone && react-native run-ios"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloodymushroom/chipmunks.git"
  },
  "author": "charming chipmunks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bloodymushroom/chipmunks/issues"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "homepage": "https://github.com/bloodymushroom/chipmunks#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "faker": "^3.1.0",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-eslint": "^7.1.1",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "grunt": "^1.0.1",
    "jquery": "^3.1.1",
    "lodash": "^4.17.3",
    "mobx": "^2.7.0",
    "mobx-react": "^4.0.4",
    "moment": "^2.17.1",
    "mysql": "^2.12.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-native": "^0.39.2",
    "react-native-cli": "^2.0.1",
    "react-router": "^3.0.0",
    "request": "^2.79.0",
    "sequelize": "^3.28.0",
    "sequelize-cli": "^2.5.1"
  }
}
